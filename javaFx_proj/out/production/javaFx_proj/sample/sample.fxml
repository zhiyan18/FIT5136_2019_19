<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600" prefWidth="400" style="-fx-background-color: #F68A1E;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TextField fx:id="searchBar" layoutX="106.0" layoutY="110.0" prefHeight="31.0" prefWidth="233.0" style="-fx-background-color: transparent; -fx-border-color: white;">
         <font>
            <Font size="13.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TextField>
      <Text fx:id="title" fill="#f8f8f8" layoutX="25.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="MonashEats" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="34.0" />
         </font>
      </Text>
      <Button fx:id="signUp" layoutX="266.0" layoutY="52.0" mnemonicParsing="false" onAction="#logInAction" style="-fx-border-width: 0px 0px 2px 0px; -fx-border-color: white;" text="Login/Signup" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
      <Button fx:id="searchButton" layoutX="43.0" layoutY="101.0" mnemonicParsing="false" onAction="#searchBtnAct" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: transparent;" styleClass="searchIcon" stylesheets="@style.css" />
      <ListView fx:id="restList" layoutX="67.0" layoutY="187.0" prefHeight="203.0" prefWidth="275.0" />
      <ButtonBar fx:id="botButtons" layoutX="59.0" layoutY="532.0" prefHeight="40.0" prefWidth="252.0">
         <buttons>
            <Button fx:id="account" mnemonicParsing="false" onAction="#accountAct" prefHeight="25.0" prefWidth="72.0" text="account">
               <graphic>
                  
               </graphic></Button>
            <Button fx:id="myOrder" mnemonicParsing="false" onAction="#myOrderAct" prefHeight="25.0" prefWidth="72.0" text="my order" />
            <Button fx:id="myCart" mnemonicParsing="false" prefHeight="25.0" prefWidth="72.0" text="my cart" />
         </buttons>
      </ButtonBar>
      <Button fx:id="showMenuBtn" layoutX="163.0" layoutY="464.0" mnemonicParsing="false" onAction="#showMenuAct" prefHeight="31.0" prefWidth="75.0" text="showMenu" />
      <TextField layoutX="68.0" layoutY="407.0" prefHeight="40.0" prefWidth="273.0" />
   </children>
</AnchorPane>
