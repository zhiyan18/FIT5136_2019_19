<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600" prefWidth="400" style="-fx-background-color: #F68A1E;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.signUpController">
   <children>
      <Text fx:id="title" fill="#f8f8f8" layoutX="25.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="MonashEats" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="34.0" />
         </font>
      </Text>
      <TextField fx:id="userName" layoutX="95.0" layoutY="120.0" prefHeight="32.0" prefWidth="211.0" text="User Name" />
      <TextField fx:id="passWord" layoutX="95.0" layoutY="334.0" prefHeight="32.0" prefWidth="211.0" text="Password" />
      <Button fx:id="signUpBtn" layoutX="141.0" layoutY="475.0" mnemonicParsing="false" onAction="#signUpBtnEvent" prefHeight="32.0" prefWidth="119.0" text="sign Up" />
      <TextField fx:id="rePassWord" layoutX="95.0" layoutY="387.0" prefHeight="32.0" prefWidth="211.0" text="Repeat Password" />
      <TextField fx:id="phoneNo" layoutX="95.0" layoutY="179.0" prefHeight="32.0" prefWidth="211.0" text="Phone number" />
      <TextField fx:id="homeAdd" layoutX="95.0" layoutY="231.0" prefHeight="32.0" prefWidth="211.0" text="Home Address" />
      <TextField fx:id="postCode" layoutX="95.0" layoutY="284.0" prefHeight="32.0" prefWidth="211.0" text="Post Code" />
   </children>
</AnchorPane>
