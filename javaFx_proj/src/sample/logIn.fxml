<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600" prefWidth="400" style="-fx-background-color: #F68A1E;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.logInController">
   <children>
      <Text fx:id="title" fill="#f8f8f8" layoutX="25.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="MonashEats" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="34.0" />
         </font>
      </Text>
      <TextField fx:id="userName" layoutX="92.0" layoutY="152.0" prefHeight="32.0" prefWidth="211.0" text="User Name" />
      <TextField fx:id="passWord" layoutX="95.0" layoutY="258.0" prefHeight="32.0" prefWidth="211.0" text="Password" />
      <CheckBox fx:id="remember" layoutX="102.0" layoutY="327.0" mnemonicParsing="false" text="remember me" />
      <Button fx:id="logInBtn" layoutX="138.0" layoutY="377.0" mnemonicParsing="false" onAction="#logInBtnAction" prefHeight="32.0" prefWidth="119.0" text="log in" />
      <Button fx:id="signUpBtn" layoutX="138.0" layoutY="443.0" mnemonicParsing="false" onAction="#signUpAction" prefHeight="32.0" prefWidth="119.0" text="sign Up" />
   </children>
</AnchorPane>
